{
  "name": "Inspired Cooking Development",
	"forwardPorts": [
		3000
	],
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/workspace",
	"shutdownAction": "stopCompose",
	"settings": {
		"terminal.integrated.shell.linux": "/bin/bash"
	},
	"extensions": [
		"rebornix.ruby",
		"editorconfig.editorconfig",
		"castwide.solargraph"
	],
	"postStartCommand": "ruby .devcontainer/onstart.rb",
	"remoteUser": "vscode"
}
